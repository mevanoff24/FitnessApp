<%= render 'layouts/navbar' %>
<h1>All Made Workouts</h1>

<%= link_to "Create Workout Routine", new_user_workout_path(@user) %>

<p><% @user_workouts.each do |item| %>
	<div>Title: <%= item.title %></div>
		<div>Description: <%= item.description %></div>
		<% item.routines.each do |rout|%>
		<div>Exercise: <%= rout.name %></div>
		<% counter = 0 %>
		<div>Reps: <%= rout.reps[counter] %></div>
		<div>Weight: <%= rout.name[counter] %></div>
		<% counter += 1 %>
		<% end %>
	<%= link_to "Show", user_workout_path(@user, item) %> |
	<%= link_to "Update", edit_user_workout_path(@user, item) %>
<% end %>
</p>
</br>